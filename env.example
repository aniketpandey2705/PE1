# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=ap-south-1

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Server Configuration
PORT=5000
# Base URL the React app will call. Typically http://localhost:5000/api in local dev.
REACT_APP_API_URL=http://localhost:5000/api

# Comma-separated list of allowed browser origins for CORS (no spaces)
# Example: http://localhost:3000,http://127.0.0.1:3000
CLIENT_ORIGIN=http://localhost:3000

# Max upload size in bytes (default 25MB)
MAX_FILE_SIZE_BYTES=26214400

# Database Configuration (if using a database later)
# DATABASE_URL=your_database_url_here